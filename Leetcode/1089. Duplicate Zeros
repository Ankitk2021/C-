class Solution {
public:

// Time complexity = O(n2)
    void duplicateZeros(vector<int>& arr) {



        int n = arr.size();
        int curr;

        for (int i = 0; i < n; i++) { // O(n)
            curr = arr[i];
            if (curr == 0){

                arr.insert(arr.begin()+i,0);  //O(n)
                i++;
            }
        }

        arr.resize(n);
    }
};
