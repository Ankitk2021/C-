class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n = nums.size()-1;
        int size = nums.size();

        vector<int> prefix(size);
        vector<int> ans(size);

        prefix[0] = 1;
        for(int i = 1; i <= n ; i++){
            prefix[i] = (prefix[i-1])*(nums[i-1]);
        }

        vector <int> suf(size);
        suf[n] = 1;

        for(int i = n-1; i >=0 ; i--){
            suf[i] = suf[i+1]*nums[i+1];
        }

        for(int i = 0 ; i <= n ; i++){
            ans[i] = prefix[i] * suf[i];
        }

        return ans;
        
    }
};
