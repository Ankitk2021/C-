    bool detectLoop(Node* head) {
        // code here
        if(head == NULL) return false;
        map<Node*,int> mpp;
        
        Node* temp = head;
        
        while(temp != NULL){
            mpp[temp]++;
            if(mpp[temp]>1) return true;
            
            temp = temp->next;
        }
        
        return false;
        
    }
