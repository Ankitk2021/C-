    Node* removeDuplicates(Node* head) {

        // code here
        if(!head) return NULL;
        Node* curr = head;
       
        while(curr && curr->next){
         
         if(curr->data == curr->next->data){
             Node* toDelete = curr->next;
             curr->next = toDelete->next;
             delete(toDelete);
            
         }else{
             curr = curr->next;
         }
        }
        
        return head;
    }
