#include <bits/stdc++.h> 
void insertionSort(int n, vector<int> &arr){
    // Write your code here.


    //outer loop
    for(int i = 1;i<n;i++){
        int temp = arr[i];
        int j;
        for(j=i-1;(arr[j]>temp && j>=0);j--){
            arr[j+1] = arr[j];

        }
        arr[j+1] = temp;
    }
    //print the array
    //  for(int x : arr){
    //      cout << x << " ";
    //  }
    // cout << endl;
}
