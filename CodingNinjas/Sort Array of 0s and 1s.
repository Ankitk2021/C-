#include <bits/stdc++.h> 


int countZero(int n, vector<int> &A){
    int s = 0;
    for(int i = 0; i < n ; i++ ){
        if(A[i]==0){
            s++;
        }
    }
    return s;
}

void sort0and1(int n, vector<int> &A){
    // Write your code here.

    int z = countZero(n,A);
    for(int i = 0 ; i < n ; i++){
        if(z != 0){
            A[i] = 0;
            z--;
        }else {
              A[i] = 1;
        }
    }
}
