#include <bits/stdc++.h>
#include <set>
#include <vector>

vector<int> ninjaAndSortedArrays(vector<int>& arr1, vector<int>& arr2, int m, int n) {
	// Write your code here.

	vector<int> v;

	for(int x : arr1){
		if(x != 0 ){
			v.push_back(x);
		}
	}

	for(int y : arr2){

		if( y != 0){
			v.push_back(y);
		}
	}

	sort(v.begin(),v.end());
	return v;

}
