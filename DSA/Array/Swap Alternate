#include <bits/stdc++.h>
using namespace std;


void print(int arr[],int n){
    
    for(int i=0;i<n;i++){
        
        cout << arr[i] << " ";
    }
}

void swapAlt(int arr[],int n){
    
    
    int f = 0;
    int b = n-1;
    
    
    
    while(f<=b){
        
        
      
        if(f%2 ==0 ){
            
             swap(arr[f],arr[b]);
                     f++;
                     b--;
        } else {
                    f++;
                    b--;
        }
       
    }
}


int main() {
	// your code goes here
	
	int arr[] = {2,3,4,5,6};
	swapAlt(arr,5);
	print(arr,5);
	
	return 0;

}
